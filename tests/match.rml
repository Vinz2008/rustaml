let match_switch1 a = match a with
  | 1 -> "hello"
  | 2 -> "world"
  | _ -> "test" ;;

let match_switch2 a = match a with
  | 1 -> 12
  | 2 -> 36
  | a -> a ;;

let m1 = match_switch1 (rand ()) ;;
let _ = print m1 ;;

let m2 = match_switch2 (rand ()) ;;
let _ = print m2 ;;

let match_bool a = match a with
  | true -> 12
  | false -> 6 ;;

let m_b = match_bool ((rand ()) == 3) ;;
let _ = print m_b ;;