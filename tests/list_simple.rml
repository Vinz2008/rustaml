let big_list n : int -> list[int] = 
  if n == 0 then []
  else n :: big_list (n - 1) ;;

let list = big_list 10 ;;


let sum l = match l with 
  | [] -> 0
  | e :: li -> e + sum li ;;

let sum_f l : list[float] -> float = match l with 
  | [] -> 0.0
  | e :: li -> e +. sum_f li ;;

let s = sum list ;;

let s_f = sum_f [0, 1, 2, 3, 4, 5, 6, 7] ;;

let a = "hello" ^ "world" ;;

